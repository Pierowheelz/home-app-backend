<?xml version="1.0" encoding="utf-8"?>
<Container>
<Beta>true</Beta>
<Category>HomeAutomation:</Category>
<Date>2022-05-29</Date>
<Changes>
[b]29.May.2022:[/b]Added[br]
</Changes>
<Support>http://forums.unraid.net</Support>
<Name>HomeAppBackend</Name>
<Description>
HomeAppBackend controller (full description on Github).
</Description>
<Registry>https://hub.docker.com/r/pierowheelz/home-app-backend</Registry>
<GitHub>https://github.com/Pierowheelz/home-app-backend</GitHub>
<Repository>pierowheelz/home-app-backend</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
  <Mode>bridge</Mode>
  <Publish>
    <Port>
      <HostPort>3600</HostPort>
      <ContainerPort>3600</ContainerPort>
      <Protocol>tcp</Protocol>
    </Port>
  </Publish>
</Networking>
<Data>
    <Volume>
        <HostDir>/usr/src/home-app-backend</HostDir>
        <ContainerDir>/home-app-backend</ContainerDir>
        <Mode>rw</Mode>
    </Volume>
    <Volume>
        <HostDir>/mnt/user/appdata/swag/etc/letsencrypt/</HostDir>
        <ContainerDir>/keys</ContainerDir>
        <Mode>r</Mode>
    </Volume>
    <Volume>
        <HostDir>/mnt/user/appdata/home-app-backend/</HostDir>
        <ContainerDir>/config</ContainerDir>
        <Mode>r</Mode>
    </Volume>
</Data>
<WebUI>http://[IP]:[PORT:3800]</WebUI>
<Banner>http://i.imgur.com/zXpacAF.png</Banner>
<Icon>http://i.imgur.com/zXpacAF.png</Icon>
<TemplateURL>https://raw.githubusercontent.com/pierowheelz/home-app-backend/master/home-app-backend.xml</TemplateURL>
</Container>
